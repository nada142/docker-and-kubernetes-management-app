<style>
  /* Chatbot Container */
  #chatbot-container {
      position: fixed;
      bottom: 20px;
      right: 20px;
      z-index: 1000;
  }

  /* Chatbot Toggle Button (Circle with Logo) */
  #chatbot-toggle {
      width: 70px;
      height: 70px;
      background: #1e90ff;
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
  }

  #chatbot-toggle img {
      width: 50px;
      height: 50px;
  }


/* Chatbot Styling */
#chatbot-interface {
  position: fixed;
  bottom: 20px;
  right: 20px;
  width: 450px;
  background: #f8f9fa;
  border-radius: 12px;
  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
  overflow: hidden;
  font-family: Arial, sans-serif;
}

/* Chat Header */
#chat-header {
  background: #1e90ff;
  color: white;
  padding: 15px;
  font-size: 16px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}

#chat-header img {
  width: 50px;
  height: 50px;
  margin-right: 10px;
}

#chat-header strong {
  display: flex;
  align-items: center;
}

#chat-close {
  background: none;
  border: none;
  color: white;
  font-size: 20px;
  cursor: pointer;
}

/* Chat Body */
#chat-body {
  max-height: 300px;
  overflow-y: auto;
  padding: 10px;
  background: white;
}

/* Chat Messages */
.chat-message {
  background: #e9ecef;
  padding: 10px;
  border-radius: 8px;
  margin-bottom: 8px;
  width: fit-content;
  max-width: 80%;
  font-size: 14px;
}

.bot-message {
  background: #d1ecf1;
  color: #0c5460;
  align-self: flex-start;
}

/* Welcome Message Styling */
.welcome-message {
  background: #1e90ff;
  color: white;
  cursor: pointer;
  text-align: center;
  padding: 10px;
  border-radius: 8px;
  font-weight: bold;
}

.welcome-message:hover {
  background: #0056b3;
}

.hidden-content {
  display: none; 
  margin-top: 5px;
  background: white;
  padding: 8px;
  border-radius: 5px;
  font-size: 14px;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
}

/* Chat Input */
#chat-input {
  display: flex;
  padding: 10px;
  background: #f8f9fa;
  border-top: 1px solid #ddd;
}

#chat-message {
  flex-grow: 1;
  padding: 8px;
  border-radius: 6px;
  border: 1px solid #ddd;
  font-size: 14px;
}

#chat-send {
  background: #007bff;
  color: white;
  border: none;
  padding: 8px 12px;
  margin-left: 5px;
  border-radius: 6px;
  cursor: pointer;
}

#chat-send:hover {
  background: #0056b3;
}
.nav-item i {
    color: rgba(255, 255, 255, 0.937) !important; 
    font-size: 18px;
}


/* Help icon base style */
.help-icon {
  cursor: help;
  display: inline-block;
  margin-left: 4px;
  position: relative;
}

.help-icon::after {
  content: attr(data-tooltip);
  position: absolute;
  top: 100%;        
  left: 50%;
  transform: translateX(-50%);
  background: #333;
  color: white;
  padding: 5px 10px;
  border-radius: 4px;
  font-size: 14px;
  width: 400px;
  opacity: 0;
  visibility: hidden;
  transition: all 0.2s;
  z-index: 100;   
 
}



.help-icon:hover::after {
  opacity: 1;
  visibility: visible;
  top: calc(100% + 5px);  
}

.input-group .help-icon {
  background: #f8f9fa;
  border-left: 1px solid #ced4da;
  padding: 0 10px;
}
